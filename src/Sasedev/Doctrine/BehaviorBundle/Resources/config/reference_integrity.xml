<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="sasedev_doctrine_behavior.listener.reference_integrity.class">Sasedev\Doctrine\Behavior\ReferenceIntegrity\ReferenceIntegrityListener</parameter>
    </parameters>
    <services>
        <service id="sasedev_doctrine_behavior.listener.reference_integrity" class="%sasedev_doctrine_behavior.listener.reference_integrity.class%" public="false">
            <call method="setAnnotationReader">
                <argument type="service" id="annotation_reader" />
            </call>
        </service>
    </services>
</container>
